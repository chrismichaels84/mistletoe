{
    "name": "michaels/mistletoe",
    "type": "library",
    "description": "Cron and Single Use Task Management for PHP.",
    "keywords": [
        "cron",
        "tasks"
    ],
    "homepage": "https://github.com/michaels/mistletoe",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Wilson",
            "email": "chrismichaels84@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
      "php": "~5.5|~7.0",
      "symfony/console": "~3.0",
      "mtdowling/cron-expression": "1.*"
    },
    "require-dev": {
      "phpunit/phpunit": "~5.0",
      "scrutinizer/ocular": "~1.1",
      "squizlabs/php_codesniffer": "~2.3"
    },
  "bin": [
    "bin/mt"
  ],
    "autoload": {
        "psr-4": {
            "Mistletoe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mistletoe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
